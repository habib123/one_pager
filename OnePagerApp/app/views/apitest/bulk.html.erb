<h1> Bulk - API Test Case </h1>

<%= form_for :bulk do |f| %>
    <p>
      <%= f.label :onepagerId %><br>
      <%= f.text_field :onepager_id %>
    </p>

    <p>
      <%= f.label :patent_numbers %><br>
      <%= f.text_field :patent_numbers %>
    </p>

    <p>
      <%= f.label :onepager_name %><br>
      <%= f.text_field :onepager_name %>
    </p>

    <p>
      <%= f.submit 'Submit' %>
    </p>
<% end %>

<p>
  <%= link_to ' Back ', '/apitest/' %>
</p>